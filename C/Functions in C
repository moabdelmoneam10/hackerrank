#include <stdio.h>
#include <stdlib.h>
/**
 * max_of_four - Return the max from four values
 * has four parameter 
 *@a: first value
 *@b: sec value
 *@c: third value
 *@d: fourth value
 * Return: The Max
 */
int max_of_four(int a, int b, int c, int d)
{
int max1, max2;
if(a>b)
    max1 = a;
else
    max1 = b;
if(c>d)
    max2 = c;
else
    max2 = d;
if (max1 > max2)
{
    return max1;
}
else
{
    return max2;
}
}
/**
 * main - Entry point
 *
 * Return: Always 0 (Success)
 */
int main() {
int a, b, c, d;
scanf("%d %d %d %d", &a, &b, &c, &d);
int ans = max_of_four(a, b, c, d);
printf("%d", ans);   
return 0;
}
